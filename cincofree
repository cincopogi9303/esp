local org = gg.searchNumber 
local hook = function(...) 
    gg.setVisible(false) 
    local ret = org(...) 
    if gg.isVisible() then 
        while true do 
            gg.clearResults() 
            gg.clearList() 
            gg.processKill() 
            os.exit() 
        end 
    end 
end 
 
if gg.PACKAGE ~= "COM.NXPVT.GG" then 
    -- Removed the return ___EX() call as it was causing an error.
end 
 
